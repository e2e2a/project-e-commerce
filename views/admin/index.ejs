
<%- include('./partials/header/header.ejs'); %>
<body class="animsition">
    <div class="page-wrapper">
        <%- include('./partials/navigation/mobile-nav.ejs'); %>
        <%- include('./partials/navigation/nav.ejs'); %>

        <!-- PAGE CONTAINER-->
        <div class="page-container">
            <%- include('./partials/sidebar/sidebar.ejs'); %>
            <!-- END HEADER DESKTOP-->

            <!-- STATISTIC-->
            <section class="statistic m-t-75">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <h2 class="m-b-20">Products</h2>
                        <div class="row">
                            <div class="col-md-6 col-lg-6">
                                <div class="statistic__item">
                                    <h2 class="number"><%= products.length %></h2>
                                    <span class="desc">total products</span>
                                    <div class="icon">
                                        <i class="zmdi zmdi-account-o"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="statistic__item">
                                    <h2 class="number"><%= totalItemsOrdered %></h2>
                                    <span class="desc">items sold</span>
                                    <div class="icon">
                                        <i class="zmdi zmdi-shopping-cart"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END STATISTIC-->
            <!-- STATISTIC-->
            <section class="statistic">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <h2 class="m-b-20">Courses</h2>
                        <div class="row">
                            <div class="col-md-6 col-lg-6">
                                <div class="statistic__item">
                                    <h2 class="number"><%= courses.length %></h2>
                                    <span class="desc">Total Courses</span>
                                    <div class="icon">
                                        <i class="zmdi zmdi-account-o"></i>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-6 col-lg-6">
                                <div class="statistic__item">
                                    <h2 class="number"><%= enrollements.length %></h2>
                                    <span class="desc">Enrolled</span>
                                    <div class="icon">
                                        <i class="zmdi zmdi-shopping-cart"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END STATISTIC-->
            <!-- STATISTIC-->
            <section class="statistic">
                <div class="section__content section__content--p30">
                    <div class="container-fluid">
                        <h2 class="m-b-20">Users</h2>
                        <div class="row">
                            <div class="col-md-6 col-lg-6">
                                <div class="statistic__item">
                                    <h2 class="number"><%= users.length %></h2>
                                    <span class="desc">Total Users</span>
                                    <div class="icon">
                                        <i class="zmdi zmdi-account-o"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>
            <!-- END STATISTIC-->

            <section>
                <div class="container-fluid">
                    <%- include('./partials/footer/copyright.ejs'); %>
                </div>
            </section>
            <!-- END PAGE CONTAINER-->
        </div>

    </div>
</body>
<%- include('./partials/footer/footer_js.ejs'); %>
